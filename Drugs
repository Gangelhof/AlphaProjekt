
package alpha.model;


public class Drugs 
{
    
    private String name;
    private int price;
    private int availability;
    
    public Drugs(String name, int price, int availability)
    {
       this.name = name;
       this.price = price;
       this.availability = availability;
    }
    
    public String getName()
    {
       return name;
    }
    
    public int getPrice()
    {
       return price;
    }
    
    public int getAvailability()
    {
       return availability;
    }
    
    public void setPrice (int price)
    {
      this.price = price;
    }
    
    public void setAvailability (int amount)
    {
      this.Availability = amount;
    }
    
}//slut
